import numpy

class GaussianBoundary(object):
    '''
    classdocs
    '''


    def __init__(self):
        '''
        Constructor
        '''
    
    def computePrior(self, data):
        ''' Assuming data is not more than 2 dimension
        '''
        [nrow, ncol] = data.shape
        print str(data.shape)
        print str(nrow)
        print str(ncol)
        count = numpy.zeros((1,ncol))
        print str(count[0][0])
        print str(data[0][0])
        
        for idx, row in enumerate(data): #all the rows
            count=0
            for idy, col in enumerate(row):
                count[0][idy] = str(count[0][idy]) + str(data[idx][idy]);
                
                
        
        print "sum:" + str(count)